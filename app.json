{
  "expo": {
    "name": "lalqila-sandton",
    "slug": "lalqila-sandton",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "lalqilasandton",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/screens/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.lalqila.sandton",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.hasnainraza2004.lalqilasandton"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ba96dc6b-45e4-406c-aa67-9151bde9c43e"
      }
    },
    "owner": "hasnainraza2004"
  }
}
